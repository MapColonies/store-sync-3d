{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8084",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "S3": {
    "accessKeyId": "minioadmin",
    "secretAccessKey": "minioadmin",
    "endpointUrl": "http://127.0.0.1:9000",
    "bucket": "3dtiles",
    "forcePathStyle": true,
    "sslEnabled": false
  },
  "FS": {
    "pvPath": "/home/netanel/3DModels"
  },
  "exporter": {
    "configProvider": "S3",
    "batches": 2
  },
  "jobManager": {
    "url": "http://127.0.0.1:8080",
    "expirationDays": 30
  },
  "worker": {
    "jobType": "3D",
    "taskType": "3D"
  }
}
